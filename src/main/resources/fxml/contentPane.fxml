                  <Button fx:id="expensesButton" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="100.0" prefWidth="110.0" text="Expenses" HBox.hgrow="ALWAYS" />
